{
  "name": "react-template",
  "version": "1.0.176",
  "description": "A template for creating a new ReactJS project",
  "author": "David LeBlanc <github@david-leblanc.com>",
  "license": "MIT",
  "repository": "git@github.com:d-leb/react-template",
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "analyze": "node scripts/analyze.mjs",
    "build": "node scripts/build.mjs",
    "eslint": "eslint --ext ts,tsx src --max-warnings=0",
    "eslint:fix": "eslint --ext ts,tsx src --fix",
    "lint": "yarn eslint && yarn stylelint && yarn prettier",
    "jest": "jest",
    "modules:disable": "node scripts/toggles.mjs node_modules disable",
    "modules:enable": "node scripts/toggles.mjs node_modules enable",
    "review": "yarn lint && yarn test && yarn build",
    "prettier": "prettier --check 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "start": "node scripts/start.mjs",
    "stylelint": "stylelint \"src/**/*.ts?\"",
    "stylelint:fix": "stylelint \"src/**/*.ts?\" --fix",
    "test": "yarn tsc",
    "tsc": "tsc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@axe-core/react": "^4.5.2",
    "@tanstack/react-query": "^4.20.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-intl": "^6.2.5",
    "react-is": "^18.2.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.5.0",
    "styled-components": "^5.3.6",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@swc/core": "^1.3.23",
    "@swc/helpers": "^0.4.14",
    "@swc/jest": "^0.2.24",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.11.7",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@yarnpkg/sdks": "^3.0.0-rc.33",
    "browserlist": "^1.0.1",
    "compression-webpack-plugin": "^10.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "imports-loader": "^4.0.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mocha": "^10.2.0",
    "ora": "^6.1.2",
    "postcss": "^8.4.20",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "stylelint": "^14.16.0",
    "stylelint-config-concentric-order": "^5.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "svg-url-loader": "^8.0.0",
    "swc-loader": "^0.2.3",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
